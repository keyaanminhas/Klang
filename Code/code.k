DECLARE NUM1 , NUM2 , NUM3 , NUM4 : INTEGER


INPUT NUM1
DECLARE NAME : STRING
OUTPUT "THIS IS A NUMBER: ", NUM1


IF NUM1 <> 3 THEN
NUM1 <-- NUM1-1


ELSE IF NUM1 > NUM2 THEN
NUM2 <-- NUM1

ELSE
NUM1 <-- (1 + 3 -2*4)

ENDIF


NUM <-- 3.2
NAME <--      "Keyaan Minhas"

FOR COUNT <-- 0 TO -5 STEP -1

OUTPUT "HELLO" , NUM1 - 2

NEXT

WHILE X <> 5 DO
HELLO <-- "KEYAAN"
ENDWHILE

REPEAT


UNTIL X <> 5